{
  "version": 2,
  "name": "dasub-admin",
  "buildCommand": "pnpm build:ui && pnpm --filter admin build",
  "outputDirectory": "apps/admin/dist",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "vite",
  "rootDirectory": "da-submission-manager",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
